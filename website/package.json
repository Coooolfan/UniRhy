{
  "name": "unirhy-site",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "oxlint . --type-check -A all -A nursery --deny-warnings",
    "typecheck": "oxlint . --type-check -A all -A nursery --deny-warnings",
    "lint": "oxlint . --type-aware --deny-warnings",
    "lint:fix": "oxlint . --fix",
    "format": "oxfmt .",
    "format:check": "oxfmt . --check",
    "verify": "run-s lint typecheck",
    "fix": "run-s lint:fix"
  },
  "dependencies": {
    "@types/three": "^0.182.0",
    "three": "^0.182.0",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.32.0",
    "oxlint": "~1.38.0",
    "oxlint-tsgolint": "^0.10.0",
    "typescript": "~5.9.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-vue-devtools": "^8.0.5"
  },
  "packageManager": "yarn@4.12.0"
}
