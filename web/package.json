{
  "name": "unirhy-web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "oxlint . --type-check -A all -A nursery --deny-warnings",
    "typecheck": "oxlint . --type-check -A all -A nursery --deny-warnings",
    "lint": "oxlint . --type-aware --deny-warnings",
    "lint:fix": "oxlint . --fix",
    "format": "oxfmt .",
    "format:check": "oxfmt . --check",
    "verify": "run-s lint typecheck",
    "fix": "run-s lint:fix",
    "api": "node script/generate-api.js"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "lucide-vue-next": "^0.564.0",
    "pinia": "^3.0.4",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.28",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.4",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.13",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "adm-zip": "^0.5.16",
    "fs-extra": "^11.3.3",
    "jsdom": "^28.1.0",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.32.0",
    "oxlint": "~1.47.0",
    "oxlint-tsgolint": "^0.13.0",
    "temp-dir": "^3.0.0",
    "typescript": "~5.9.3",
    "uuid": "^13.0.0",
    "vite": "^8.0.0-beta.14",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": "^24.0.0"
  },
  "packageManager": "yarn@4.12.0"
}
